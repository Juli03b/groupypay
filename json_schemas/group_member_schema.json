{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Group Member Schema",
    "description": "For group members",
    "default": {},
    "examples": [
        {
            "name": "Julio",
            "email": "julio@gmail.com",
            "phone_number": "9727777111"
        }
    ],
    "required": [
        "name",
        "email"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Julio"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "julio@gmail.com"
            ]
        },
        "phone_number": {
            "$id": "#/properties/phone_number",
            "type": "string",
            "title": "The phone_number schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "9727777111"
            ]
        }
    },
    "additionalProperties": true
}